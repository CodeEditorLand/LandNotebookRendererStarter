{
	"name": "vscode-notebook-renderer-starter",
	"displayName": "vscode-notebook-renderer-starter",
	"version": "0.0.1",
	"private": false,
	"description": "Notebook renderer starter template",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandNotebookRendererStarter#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandNotebookRendererStarter/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandNotebookRendererStarter.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "npm run compile:extension && npm run compile:client",
		"compile:client": "webpack --info-verbosity verbose --mode production",
		"compile:extension": "tsc -b",
		"dev": "concurrently -r npm:watch:extension npm:dev:client",
		"dev:client": "webpack-dev-server",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"updatetypes": "cd src/extension/types && vscode-dts dev && vscode-dts master",
		"watch:client": "webpack --info-verbosity verbose --mode development --watch",
		"watch:extension": "tsc -b --watch"
	},
	"contributes": {
		"notebookOutputRenderer": [
			{
				"displayName": "Sample Notebook Renderer",
				"entrypoint": "./out/client/index.js",
				"mimeTypes": [
					"application/json"
				],
				"viewType": "sample-notebook-renderer"
			}
		],
		"notebookProvider": [
			{
				"displayName": "CPU Profile Notebook",
				"selector": [
					{
						"filenamePattern": "*.sample"
					}
				],
				"viewType": "sample-notebook-renderer"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.10",
		"concurrently": "8.2.2",
		"css-loader": "7.1.1",
		"glob": "10.3.12",
		"style-loader": "4.0.0",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"enableProposedApi": true
}
